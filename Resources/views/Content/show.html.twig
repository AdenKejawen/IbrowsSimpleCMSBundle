<h1>{{type}} Content</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Keyword</th>
            <td>{{ entity.keyword }}</td>
        </tr>
        <tr>
            <th>Html</th>
            <td>{{ entity.keyword|scms(type) }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('ibrows_simple_cms_content', { 'type':type }) }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('ibrows_simple_cms_content_edit', { 'id': entity.id,'type':type }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('ibrows_simple_cms_content_delete', { 'id': entity.id,'type':type }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
